	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Root Context: defines shared resources visible to all other web components -->
		<bean id="UserController" class="com.me.controller.UserController"></bean>
		<bean id="userdao" class="com.me.dao.UserDao"></bean>
		<bean id="productdao" class="com.me.dao.ProductDao"></bean>
		<bean id="cartitemdao" class="com.me.dao.CartItemDao"></bean>
		<bean id="cartdao" class="com.me.dao.CartDao"></bean>
		<bean id="validation" class="com.me.validator.Validation"></bean>
